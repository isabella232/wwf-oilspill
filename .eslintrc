{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "amd": true
  },
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "indent": ["error", 2],
    "quotes": [
      "error",
      "single"
    ]
  }
}
